extends ../shared/template

block content
  .row
    .small-12.columns
      #container
        if user
          #tabWrap
            h1= user.name
            h2= user.email
            h3#progressPics: a(href='/user/progress') Progress Pics
            if user.wods
              - for (var i=0; i<user.wods.length; i++)
                h5= user.wodsCompleted[i].name
                h5= user.wods[i].date


